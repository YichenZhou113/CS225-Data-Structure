#include "HSLAPixel.h"

namespace cs225{

 HSLAPixel::HSLAPixel(){
  h = 0;
  s = 1.0;
  l = 1.0;
  a = 1.0;
 }

 HSLAPixel::HSLAPixel (double hue, double saturation, double luminance)
 {
  this->h = hue;
  this->s = saturation;
  this->l = luminance;
 }

 HSLAPixel::HSLAPixel (double hue,
		             double saturation,
		             double luminance,
                             double alpha
	                    ) {
                        this->h = hue;
                        this->s = saturation;
                        this->l = luminance;
                        this->a = alpha;
                      }
}
